{
  "name": "HelloWorld",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext .js,.ts,.tsx",
    "postinstall": "react-native link",
    "start:android": "jetify && react-native run-android",
    "start:ios": "(cd ios && pod install) && react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "type-check": "tsc --noEmit -p . --pretty",
    "prestorybook": "rnstl",
    "storybook": "(adb reverse tcp:7007 tcp:7007 || true) && start-storybook -p 7007"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn type-check && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.6.2",
    "i18next": "17.2.0",
    "ramda": "0.26.1",
    "react": "16.10.2",
    "react-i18next": "10.13.1",
    "react-native": "0.61.2",
    "react-native-gesture-handler": "1.4.1",
    "react-native-localize": "1.3.0",
    "react-native-reanimated": "1.3.0",
    "react-native-screens": "1.0.0-alpha.23",
    "react-navigation": "4.0.10",
    "react-navigation-stack": "1.9.4",
    "react-navigation-tabs": "2.5.6",
    "react-redux": "7.1.1",
    "redux": "4.0.4",
    "redux-starter-kit": "0.8.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/runtime": "7.6.3",
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@react-native-community/eslint-config": "0.0.5",
    "@storybook/addon-actions": "5.2.4",
    "@storybook/addon-knobs": "5.2.4",
    "@storybook/addon-links": "5.2.4",
    "@storybook/addon-ondevice-actions": "5.2.4",
    "@storybook/addon-ondevice-knobs": "5.2.4",
    "@storybook/addons": "5.2.4",
    "@storybook/react-native": "5.2.4",
    "@storybook/react-native-server": "5.2.4",
    "@types/jest": "24.0.19",
    "@types/react": "16.9.6",
    "@types/react-native": "0.60.19",
    "@types/react-redux": "7.1.4",
    "@types/react-test-renderer": "16.9.1",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "eslint": "6.5.1",
    "eslint-config-prettier": "6.4.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.1",
    "husky": "3.0.9",
    "jest": "24.9.0",
    "lint-staged": "9.4.2",
    "metro-react-native-babel-preset": "0.56.0",
    "prettier": "1.18.2",
    "react-dom": "16.10.2",
    "react-native-storybook-loader": "1.8.1",
    "react-test-renderer": "16.10.2",
    "reactotron-react-native": "4.0.2",
    "typescript": "3.6.4"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.stories.tsx",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
